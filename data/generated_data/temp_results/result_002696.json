    ".txt",
    ".jpeg",
    ".html",
    ".php",
    ".config",
    ".bashrc",
    ".profile",
    ".md",
    ".json",
    ".csv",
    ".docx",
    ".xlsx",
    ".pptx",
    ".log",
    ".bash",
    ".vimrc",
    ".gitignore",
    ".tar.gz",
    ".7z",
    ".rar",
    ".mp3",
    ".mp4",
    ".wav",
    ".flac",
    ".avi",
    ".mkv",
    ".mov",
    ".exe",
    ".dll",
    ".bat",
    ".sh",
    ".c",
    ".cpp",
    ".java",
    ".py",
    ".rb",
    ".go",
    ".rs",
    ".js",
    ".ts",
    ".xml",
    ".yml",
    ".yaml",
    ".ini",
    ".cfg",
    ".toml",
    ".lock",
    ".sql",
    ".db"
