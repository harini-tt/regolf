  "no": [
    "",
    "no_extension",
    "file.",
    ".",
    "file..hidden",
    "/path/to/file.txt",
    "C:\\path\\to\\file.txt",
    ".hidden/file",
    "file.name.ext/",
    ".file/",
    "file.name./hidden",
    ".file.with.dot.",
    "directory/.file",
    ".file.with.dots.",
    "file/with.extension",
    "file.with\\backslash",
    "file.with.forward/slash",
    ".file.with.multiple.dots.",
    "file.with.multiple.dots.",
    ".hidden_folder/file",
    "\\.hidden\\file",
    "file.with.dot./",
    "/.hidden",
    "./hidden",
    "/.hidden/",
    "file.with.trailing.dot.",
    ".hidden.with.trailing.dot.",
    "file\\with\\backslash",
    "/file/with/slash",
    "file/with/trailing/slash/",
    "C:\\path\\to\\.hidden",
    ".file/with/slash",
    ".file\\with\\backslash",
    ".file.with.dot\\",
    "file\\with\\dots\\",
    ".file.with.many.dots",
    ".file.with.many.dots/",
    ".file/hidden/",
    ".hidden.file/",
    "/path/.hidden",
    "file.name/",
    "file.name\\",
    ".hidden\\file\\",
    "file.with.dot\\name"
  ]
}