{
  "expr": "^(?!\\s*$)\\s*(?!\\s)([^#\\n\\[=]+?)\\s*?(?>#.*?$|(?>=\\s*$|=\\s*?(?:#?|(\".*?\"|[^\\s].*?)\\s*?)(?:\\s*#.*?$)*?$)?$)",
  "yes": [
    "skip-grant-tables",
    "bind-address=192.168.0.1",
    "port = 3306",
    "sql-mode=\"STRICT_TRANS_TABLES,NO_AUTO_CREATE_USER\"",
    "log-bin=mysql-bin",
    "server-id=1 # identifier",
    "log-error=/var/log/mysql/error.log",
    "datadir     = /var/lib/mysql/",
    "socket = /var/run/mysqld/mysqld.sock",
    "pid-file=/var/run/mysqld/mysqld.pid",
    "innodb-buffer-pool-size=1G",
    "mysqld = /usr/sbin/mysqld",
    "skip-name-resolve",
    "query_cache_size = 16M",
    "thread_cache_size=8",
    "open_files_limit=2000",
    "join_buffer_size = 256K",
    "sort_buffer_size=256K",
    "read_rnd_buffer_size=512K",
    "sql_mode=\"NO_ENGINE_SUBSTITUTION\"",
    "default-time-zone='+00:00'",
    "character-set-server=utf8mb4",
    "collation-server=utf8mb4_unicode_ci",
    "bind-address=123.456.789.101 # comment",
    "key_buffer_size=16M",
    "myisam-recover-options=BACKUP",
    "max_allowed_packet=16M",
    "max_connect_errors=100",
    "table_open_cache=2000",
    "innodb-log-file-size=50M",
    "innodb_log_buffer_size=8M",
    "innodb_flush_log_at_trx_commit=1",
    "innodb_file_per_table",
    "innodb_open_files=400",
    "innodb_io_capacity=200",
    "innodb_flush_method=O_DIRECT",
    "log-slow-queries=/var/log/mysql/mysql-slow.log",
    "long_query_time=2",
    "log-queries-not-using-indexes",
    "expire_logs_days=10",
    "sync_binlog=1",
    "tmp_table_size=32M",
    "max_heap_table_size=32M",
    "slow_query_log=1",
    "slow_query_log_file=/var/log/mysql/mysql-slow.log",
    "innodb_read_io_threads=4",
    "innodb_write_io_threads=4",
    "innodb_buffer_pool_instances=1",
    "back_log=50",
    "slave-net-timeout=60",
    "master-info-repository=TABLE",
    "relay-log-info-repository=TABLE"
  ],
  "no": [
    "",
    " ",
    "# just a comment",
    "   # indented comment",
    "========",
    "[]",
    "[section]",
    "; comment",
    "key only",
    "value only =",
    "= no key",
    "key =  value # comment but no value",
    "key= \"unterminated string",
    "key= value # comment \n with newline",
    "key==value",
    "key = value # comment \n new line",
    "key # comment only",
    "key # comment \n with newline",
    "key=value#comment without space",
    "key =  value #comment without space",
    "=value",
    "key # comment with # multiple #",
    "key=value ## double hash",
    "key = value # comment with invalid#=character",
    "key = \"string with # inside\" # comment",
    "key = \"unclosed string",
    "\t# tabbed comment",
    "key = value # comment with \n newline",
    "key = \"value with spaces inside\"",
    "key = value # comment with spaces \n and newlines",
    " key = value # leading space",
    "key = value # trailing space ",
    "key=value#comment at end",
    "key=value # comment#with#hashes",
    "key=value # comment with = equals inside",
    "  ",
    "\n",
    "\t",
    "key = \"value with \"escaped\" quotes\"",
    "key = \"value with \"escaped quotes\"",
    "key = \"value with escaped quotes\"\"",
    "key = \"unterminated value",
    "key = \"value\" # unclosed comment",
    "key = \"value with # hash\" # comment"
  ]
}