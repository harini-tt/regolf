    "opacity: 0.5;",
    "z-index: 10;",
    "top: 50%;",
    "left: 50%;",
    "position: absolute;",
    "display: block;",
    "visibility: hidden;",
    "overflow: hidden;",
    "text-align: center;",
    "vertical-align: middle;",
    "white-space: nowrap;",
    "clip: rect(1px, 1px, 1px, 1px);",
    "transform: translate(-50%, -50%);",
    "letter-spacing: 0.1em;",
    "word-spacing: 0.2em;",
    "line-height: 1.5;",
    "min-height: 100vh;",
    "direction: rtl;",
    "cursor: pointer;",
    "outline: none;",
    "fill: #000;",
    "stroke: #fff;",
    "filter: blur(5px);",
    "float: left;",
    "clear: both;",
    "flex: 1;",
    "order: 2;",
    "grid-template-columns: repeat(3, 1fr);",
    "justify-content: space-between;",
    "align-items: flex-start;",
    "gap: 10px;"
